<% model.custom_attributes.each do | custom_attribute | %>
  <div class="flex flex-col col-span-full">
    <label class="typography-text-s-lh150 text-gray-pallete-p3">
      <%= model.custom_attribute_display_name(custom_attribute[0]) %>
    </label>
    <p class="typography-sub-text-m-lh150 text-dark-gray-palette-p1">
      <%= custom_attribute[1] %>
    </p>
  </div>
<% end %>
